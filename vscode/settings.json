  "security.workspace.trust.untrustedFiles": "open",
  "svelte.enable-ts-plugin": true,
  "terminal.external.osxExec": "iTerm 2.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.fontFamily": "'JetBrains Mono Thin', 'SFMono-Light', 'InputMono-Light', Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "200",
  "terminal.integrated.fontWeightBold": "300",
  "terminal.integrated.ignoreProcessNames": [
    "starship",
    "oh-my-posh",
    "bash",
    "zsh",
    "fish"
  ],
  "terminal.integrated.lineHeight": 1.12,
  "terminal.integrated.persistentSessionScrollback": 300,
  "terminal.integrated.profiles.osx": {
    "bash (homebrew)": {
      "args": [
        "-l"
      ],
      "icon": "terminal-bash",
      "path": "/opt/homebrew/bin/bash"
    },
    "fish": {
      "args": [
        "-l"
      ],
      "path": "fish"
    },
    "zsh (homebrew)": {
      "path": "/opt/homebrew/bin/zsh"
    }
  },

{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[fish]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.formatOnSave": true
  },
  "breadcrumbs.enabled": true,
  "cursor.cpp.disabledLanguages": [
    "plaintext",
    "scminput"
  ],
  "debug.console.fontFamily": "'JetBrains Mono Thin', 'SFMono-Light', 'InputMono-Light', Monaco, 'Courier New', monospace",
  "debug.console.fontSize": 20,
  "editor.find.cursorMoveOnType": false,
  "editor.fontFamily": "'JetBrains Mono Thin', 'SFMono-Light', 'InputMono-Light', Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.guides.indentation": false,
  "editor.inlineSuggest.enabled": true,
  "editor.lineNumbers": "on",
  "editor.matchBrackets": "near",
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.quickSuggestions": {
    "comments": false,
    "other": true,
    "strings": false
  },
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    110
  ],
  "editor.showFoldingControls": "always",
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordSeparators": "./\\()\"':,.;<>~!@#$%^&*|+=[]{}`~?",
  "editor.wordWrap": "on",
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "explorer.autoRevealExclude": {
    "**/bower_components": true,
    "**/node_modules": true
  },
  "explorer.confirmDelete": false,
  "files.associations": {
    ".env.*": "dotenv",
    ".inputrc": "shellscript",
    ".zpreztorc": "shellscript",
    "*.css": "tailwindcss",
    "*.dockerignore": "plaintext",
    "*.inky-haml": "haml",
    "*.tsx": "typescriptreact",
    "prompt_maxrelax_setup": "shellscript",
    "prompt_smiley_setup": "shellscript",
    ".cursorrules": "plaintext"
  },
  "files.encoding": "utf8",
  "files.exclude": {
    "*.sublime-*": true,
    "**/__pycache__": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.pytest_cache": true,
    "**/node_modules": true,
    "env*": true,
    "venv": true
  },
  "files.hotExit": "off",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/env/**": true,
    "**/node_modules/**": true,
    "**/venv/**": true,
    "env-*": true
  },
  "git.autofetch": true,
  "git.openRepositoryInParentFolders": "always",
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.enable": {
    "*": true,
    "javascript": true,
    "markdown": true,
    "plaintext": false,
    "scminput": false,
    "yaml": true
  },
  "githubPullRequests.pullBranch": "never",
  "gitlens.advanced.fileHistoryFollowsRenames": true,
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "gitlens.blame.heatmap.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.codeLens.scopes": [
    "document",
    "containers"
  ],
  "gitlens.currentLine.enabled": false,
  "gitlens.defaultDateFormat": "YYYY-MM-DD [at] H:mma",
  "gitlens.defaultGravatarsStyle": "retro",
  "gitlens.keymap": "alternate",
  "gitlens.statusBar.command": "gitlens.toggleFileBlame",
  "gitlens.statusBar.reduceFlicker": true,
  "javascript.preferences.quoteStyle": "single",
  "javascript.referencesCodeLens.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "markdown-preview-enhanced.liveUpdate": false,
  "markdown.extension.tableFormatter.normalizeIndentation": true,
  "markdown.preview.fontSize": 20,
  "openInGitHub.defaultPullRequestBranch": "main",
